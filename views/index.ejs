<%- include("partials/header") %>

<h1>These are the workouts</h1>

<% workouts.map(workout => { %>
    <div class="ui container">
        <h2><%= workout.title %> </h2>
        <iframe width="420" height="315"
        src='<%= workout.video %>'></iframe>
        <p><%= workout.body %> </p>
        <p>Date created: <%= workout.created %></p>
    </div>
<% }) %> 

<%- include("partials/footer") %>